<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_product_list_list_washburn" model="ir.ui.view">
        <field name="name">product.list.list.washburn</field>
        <field name="model">product.list</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_product_list_form_washburn" model="ir.ui.view">
        <field name="name">product.list.form.washburn</field>
        <field name="model">product.list</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <field name="name"/>
                    <notebook>
                        <page string="Products">
                            <field name="product_ids">
                                <tree>
                                    <field name="name"/>
                                    <field name="invoice_policy"/>
                                    <field name="standard_price"/>
                                    <field name="qty_available"/>
                                    <field name="virtual_available"/>
                                    <field name="uom_id"/>
                                </tree>
                            </field>
                        </page>
                        <page string="Customers">
                            <field name="partner_ids">
                                <tree>
                                    <field name="name"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <record id="open_product_list_action" model="ir.actions.act_window">
        <field name="name">Product List</field>
        <field name="res_model">product.list</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem
            id="menu_board_my_dash"
            parent="sale.product_menu_catalog"
            action="open_product_list_action"
            sequence="10"/>
</odoo>